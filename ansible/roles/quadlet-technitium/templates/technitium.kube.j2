[Unit]
Description=Technitium DNS Server
After=local-fs.target

[Install]
WantedBy=multi-user.target default.target

[Service]
Restart=always

[Kube]
#AutoUpdate=registry
AutoUpdate=local
#ConfigMap=
Network=technitium.network
Yaml=technitium.yaml
UserNS=keep-id:uid={{ _tdns_uid }},gid={{ _tdns_gid }}
